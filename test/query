SELECT c_balance, c_first, c_middle, h_c_w_id, h_amount FROM customer, history WHERE h_c_d_id = c_d_id AND c_d_id = 2 AND c_balance > 14000;
SELECT c_id, c_first, c_middle FROM customer;
SELECT h_c_id, h_c_w_id FROM history;
SELECT no_d_id FROM new_orders;
SELECT i_id, i_data FROM item;
SELECT ol_number FROM order_line;
SELECT i_price, i_name, i_data FROM item WHERE i_id = 43;
SELECT c_balance, c_first, c_middle, c_last FROM customer WHERE c_w_id = 3 AND c_id = 3;
SELECT ol_o_id, ol_dist_info, i_name FROM item, order_line WHERE ol_i_id = i_id AND ol_amount > 40;
SELECT c_balance, c_first, h_c_w_id, h_amount FROM customer, history WHERE h_c_id = c_id AND c_balance > 40;
SELECT d_name, c_first, c_middle, h_c_w_id, h_amount FROM customer, history, district WHERE h_c_w_id = c_w_id AND c_d_id = d_id;
